<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wolkabout.hexiwear.activity.Range">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Current"
        app:layout_constraintLeft_toRightOf="@+id/textView"
        android:layout_marginLeft="24dp"
        android:id="@+id/textView6"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="32dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Minimum"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="32dp"
        android:layout_marginLeft="113dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Maximum"
        app:layout_constraintLeft_toRightOf="@+id/textView6"
        android:layout_marginLeft="34dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="32dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Temperature"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="65dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Humidity"
        android:layout_marginTop="36dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        android:layout_marginLeft="29dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Light"
        android:layout_marginTop="35dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        android:layout_marginLeft="40dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <EditText
        android:id="@+id/ct"
        android:layout_width="57dp"
        android:layout_height="37dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="number"
        android:text="10"
        android:textSize="16dp"
        app:layout_constraintLeft_toRightOf="@+id/tin"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <EditText
        android:id="@+id/tax"
        android:layout_width="57dp"
        android:layout_height="37dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="number"
        android:textSize="16dp"
        app:layout_constraintLeft_toRightOf="@+id/ct"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <EditText
        android:id="@+id/hin"
        android:layout_width="57dp"
        android:layout_height="37dp"
        android:layout_marginLeft="29dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="number"
        android:textSize="16dp"
        app:layout_constraintLeft_toRightOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/tin" />

    <EditText
        android:id="@+id/ch"
        android:layout_width="57dp"
        android:layout_height="37dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="number"
        android:text="10"
        android:textSize="16dp"
        app:layout_constraintLeft_toRightOf="@+id/hin"
        app:layout_constraintTop_toBottomOf="@+id/ct" />

    <EditText
        android:id="@+id/hax"
        android:layout_width="57dp"
        android:layout_height="37dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="17dp"
        android:ems="10"
        android:inputType="number"
        android:textSize="16dp"
        app:layout_constraintLeft_toRightOf="@+id/ch"
        app:layout_constraintTop_toBottomOf="@+id/tax" />

    <EditText
        android:id="@+id/lin"
        android:layout_width="57dp"
        android:layout_height="37dp"
        android:layout_marginLeft="41dp"
        android:layout_marginTop="15dp"
        android:ems="10"
        android:inputType="number"
        android:textSize="16dp"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/hin" />

    <EditText
        android:id="@+id/cl"
        android:layout_width="57dp"
        android:layout_height="37dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="number"
        android:text="10"
        android:textSize="16dp"
        app:layout_constraintLeft_toRightOf="@+id/lin"
        app:layout_constraintTop_toBottomOf="@+id/ch" />

    <EditText
        android:id="@+id/tin"
        android:layout_width="57dp"
        android:layout_height="37dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="number"
        android:textSize="16dp"
        app:layout_constraintLeft_toRightOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <EditText
        android:id="@+id/lax"
        android:layout_width="57dp"
        android:layout_height="37dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="number"
        android:textSize="16dp"
        app:layout_constraintLeft_toRightOf="@+id/cl"
        app:layout_constraintTop_toBottomOf="@+id/hax" />

    <Button
        android:id="@+id/Default"
        android:layout_width="95dp"
        android:layout_height="53dp"
        android:text="default"
        android:textSize="14sp"
        app:layout_constraintLeft_toRightOf="@+id/exit"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="141dp" />

    <Button
        android:id="@+id/exit"
        android:layout_width="95dp"
        android:layout_height="53dp"
        android:text="exit"
        android:textSize="14sp"
        app:layout_constraintLeft_toRightOf="@+id/save"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="141dp" />

    <Button
        android:id="@+id/save"
        android:layout_width="95dp"
        android:layout_height="53dp"
        android:text="save"
        android:textSize="14sp"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="141dp" />
</android.support.constraint.ConstraintLayout>